{
  "name": "dk-request",
  "author": "Dmitry Kazakov",
  "license": "MIT",
  "version": "1.0.6",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dkazakov8/dk-request.git"
  },
  "description": "Request utility with validations based on Axios & ts-interface-checker",
  "scripts": {
    "analyze:js": "eslint --ignore-path .formatignore --ext \".js,.ts,.tsx\" ./",
    "format:js": "eslint --ignore-path .formatignore --ext \".js,.ts,.tsx\" --fix",
    "build": "rm -rf ./dist&&tsc --project tsconfig-compile.json",
    "test": "nyc mocha",
    "prepublishOnly": "yarn build",
    "posttest": "nyc report --reporter=json"
  },
  "nyc": {
    "include": "src/*.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "set -e&&yarn test&&git add coverage&&lint-staged"
    }
  },
  "dependencies": {
    "axios": "0.25.0",
    "dk-checker-remove-extraneous": "1.0.2"
  },
  "devDependencies": {
    "@types/chai": "4.3.0",
    "@types/mocha": "9.1.0",
    "@types/node": "17.0.17",
    "@types/sinon": "10.0.11",
    "chai": "4.3.6",
    "dk-eslint-config": "1.1.5",
    "husky": "4.3.8",
    "lint-staged": "12.1.7",
    "mocha": "9.2.0",
    "nyc": "15.1.0",
    "nock": "13.2.4",
    "ts-node": "10.5.0",
    "typescript": "4.5.5",
    "ts-interface-checker": "1.0.2"
  },
  "peerDependencies": {
    "ts-interface-checker": "^1.0.2"
  },
  "bugs": {
    "url": "https://github.com/dkazakov8/dk-request/issues"
  },
  "homepage": "https://github.com/dkazakov8/dk-request#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": ["request"]
}
